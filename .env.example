# Google AI (for Gemini) - Required
# Get from: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your-gemini-api-key

# Twilio (for WhatsApp) - Optional, only needed for proactive messages
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_WHATSAPP_NUMBER=whatsapp-number

# Supabase PostgreSQL - Required for session storage
# Get from: Supabase Dashboard > Project Settings > Database > Connection string
DATABASE_URL=postgresql+asyncpg://postgres.PROJECT_REF:PASSWORD@aws-0-REGION.pooler.supabase.com:6543/postgres

# ElevenLabs (for Voice Agent) - Optional, for outbound voice calls
# Get from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your-elevenlabs-api-key
# Phone number ID from ElevenLabs dashboard after importing Twilio number
# Get from: https://elevenlabs.io/app/conversational-ai/phone-numbers
ELEVENLABS_PHONE_NUMBER_ID=your-imported-twilio-phone-id
# Voice ID for TTS (optional; e.g. Hans Claesen - Professional Narrator from ElevenLabs Voices)
ELEVENLABS_VOICE_ID=your-voice-id-here
# Optional overrides (defaults match ElevenLabs dashboard: GPT-OSS-120B, expressive, medium reasoning, Turbo voice)
# ELEVENLABS_LLM_MODEL=gpt-oss-120b
# ELEVENLABS_LLM_TEMPERATURE=0.65
# ELEVENLABS_LLM_REASONING_EFFORT=medium
# ELEVENLABS_LLM_MAX_TOKENS=-1
# ELEVENLABS_TTS_MODEL_ID=eleven_turbo_v2_5
# ELEVENLABS_TTS_STABILITY=0.3
# ELEVENLABS_TTS_SPEED=1.15
# ELEVENLABS_TTS_SIMILARITY_BOOST=0.35
# Webhook secret for validating ElevenLabs post-call webhooks
# Get from: https://elevenlabs.io/app/agents/settings (when setting up webhook)
ELEVENLABS_WEBHOOK_SECRET=wsec_5a59290b61995c1afaef54c788f027e74ed6516b38e61c93979b729d013d09d7